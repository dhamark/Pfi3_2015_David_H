<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.k3lara.assignment_3_starter_kit.BlankFragment">

    tools:context=".Skanetrafiken"
    android:id="@+id/fragment_rese_layout">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:weightSum="1">


        <Spinner
            android:layout_width="242dp"
            android:layout_height="wrap_content"
            android:id="@+id/avgangAvgang"
            android:spinnerMode="dropdown"
            android:entries="@array/avgangAvgang"
            />

        <Spinner
            android:layout_width="234dp"
            android:layout_height="wrap_content"
            android:id="@+id/destination"
            android:spinnerMode="dropdown"
            android:entries="@array/destination"

            />

        <ExpandableListView
            android:layout_width="wrap_content"
            android:layout_height="283dp"
            android:id="@+id/expandableListView"
            android:groupIndicator="@null"


            android:layout_weight="1.13" />


    </LinearLayout>


</FrameLayout>
